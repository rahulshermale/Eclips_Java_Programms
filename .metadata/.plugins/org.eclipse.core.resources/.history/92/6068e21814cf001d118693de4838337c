public class Extended_Programm {
	public static void main(String[] args) {
		int arr[]=new int[3];
		arr[0]=10;
		arr[1]=20;
		arr[2]=30;
//	    int i;
		for(int i=0;i<arr.length;i++) 
			System.out.println(arr[i]);}
		
	
	}

